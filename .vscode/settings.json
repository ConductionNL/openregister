{
    "files.autoSave": "afterDelay",
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "cSpell.words": [
        "depubliceren",
        "Depubliceren",
        "gedepubliceerd",
        "Matadata",
        "nextcloud",
        "opencatalogi",
        "organisation",
        "Organisation",
        "organisations",
        "Organisations",
        "pinia",
        "Toegangs"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}
